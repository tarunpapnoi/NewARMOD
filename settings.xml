<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

  <mirrors>
    <mirror>
      <id>TELUSCentral</id>
      <mirrorOf>*,!telus-build-authorized</mirrorOf>
      <url>http://mavenrepository.tsl.telus.com/nexus/content/groups/telus-unauthorized</url>
    </mirror>
  </mirrors>

  <profiles>
    <profile>
      <id>telus</id>
      <repositories>
        <repository>
          <id>telus-unauthorized</id>
          <url>http://mavenrepository.tsl.telus.com/nexus/content/groups/telus-unauthorized</url>
          <releases>
            <enabled>true</enabled>
            <checksumPolicy>warn</checksumPolicy>
            <updatePolicy>always</updatePolicy>
          </releases>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>telus-build-authorized</id>
          <url>http://mavenrepository.tsl.telus.com/nexus/content/groups/telus-build-authorized</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>telus</activeProfile>
  </activeProfiles>
  <pluginGroups>
    <pluginGroup>telus-maven.plugins</pluginGroup>
  </pluginGroups>
</settings>